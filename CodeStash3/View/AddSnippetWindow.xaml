<Window x:Class="CodeStash3.View.AddSnippetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CodeStash3.View"
        mc:Ignorable="d" Height="100" Width="178" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" KeyDown="Window_KeyDown" WindowStyle="ToolWindow">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock>New snippet name:</TextBlock>
            <TextBox x:Name="textBox" Margin="20,0,25,0"/>
            <!--<Button x:Name="okBtn" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="55,10" Width="40" Click="okBtn_Click" Content="OK"/>-->
        </StackPanel>

    </Grid>
</Window>
